{
  "name": "grouping_files",
  "version": "0.1.1",
  "description": "sort files into directories by name similarities ",
  "main": "build/index.js",
  "scripts": {
    "test": "jest ./src",
    "start": "ts-node src/index.ts"
  },
  "author": "Shunsuke Watanabe <craftgear@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.6",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^26.5.2",
    "prettier": "^1.17.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "built",
      "<rootdir>/node_modules"
    ],
    "cacheDirectory": ".jest/cache",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
